<h1>Edit</h1>
<%= form_for @post do |f| %>

<div>
  <%= f.label "Product Name" %>
  <%= f.text_field :productname %>
</div>

<div>
  <%= f.label :decription %>
  <%= f.text_area :description %>
</div>

<div>
  <%= f.label :price %>
  <%= f.text_field :price %>
</div>

<div>
  <%= f.label :location %>
  <%= f.text_field :location %>
</div>

<div>
  <%= f.label :address %>
  <%= f.text_field :address %>
</div>

<div>
  <%= f.label :city %>
  <%= f.text_field :city %>
</div>

<div>
  <%= f.label :state %>
  <%= f.text_field :state %>
</div>

<div>
  <%= f.file_field :image %>
</div>

<div>
  <%= f.submit "Submit" %>
</div>

<%= link_to "Back", posts_path %>
<% end %>
